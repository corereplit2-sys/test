{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    },
    {
      "src": "api/auth-standalone.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/data.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth-standalone.ts"
    },
    {
      "src": "/api/(admin/users|msps|qualifications)",
      "dest": "/api/data.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ]
}
